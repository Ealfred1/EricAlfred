{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/assets/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
